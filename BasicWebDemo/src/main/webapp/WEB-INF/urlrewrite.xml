<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

    <!-- Gallery -->
    <rule>
        <from>/gallery</from>
        <to>/gallery.jsp</to>
    </rule>

    <!-- Index -->
    <rule>
        <from>/index</from>
        <to>/index.jsp</to>
    </rule>


    <!-- Profile -->
    <rule>
        <from>/profile</from>
        <to>/profile.jsp</to>
    </rule>

    <!-- Search -->
    <rule>
        <from>/search</from>
        <to>/search.jsp</to>
    </rule>

    <!-- Popular -->
    <rule>
        <from>/popular</from>
        <to>/popular.jsp</to>
    </rule>

    <!-- Logout -->
    <rule>
        <from>/logout</from>
        <to>/logout.jsp</to>
    </rule>

</urlrewrite>
